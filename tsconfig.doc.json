{
    "compilerOptions": {
        "target": "es2022",
        "skipLibCheck": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "EsNext",
        "moduleResolution": "NodeNext",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noEmit": true,
        "verbatimModuleSyntax": true,
        "baseUrl": "."
    }
}
